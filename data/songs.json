            "lyrics": [
                {
                    "id": 1,
                    "text": "前奏",
                    "time": 0,
                    "type": "prelude",
                    "duration": 30.187196950408936
                },
                {
                    "id": 2,
                    "text": "少しづつほつれ始めていた日々に",
                    "time": 30.187196950408936,
                    "type": "normal"
                },
                {
                    "id": 3,
                    "text": "背を向ける弱さは",
                    "time": 33.799569607086184,
                    "type": "normal"
                },
                {
                    "id": 4,
                    "text": "包み隠して我を忘れる夜に",
                    "time": 37.071937045776366,
                    "type": "normal"
                },
                {
                    "id": 5,
                    "text": "否応無く知らされた",
                    "time": 40.38185617929077,
                    "type": "normal"
                },
                {
                    "id": 6,
                    "text": "ディストーション、ノイズプレイ、形容詞の歌",
                    "time": 43.64967387411499,
                    "type": "normal"
                },
                {
                    "id": 7,
                    "text": "見透かされた気がして",
                    "time": 47.338377641418454,
                    "type": "normal"
                },
                {
                    "id": 8,
                    "text": "スコットランド、グラスゴー、雨降りの夢",
                    "time": 50.52421599237061,
                    "type": "normal"
                },
                {
                    "id": 9,
                    "text": "繰り返しては消える",
                    "time": 54.13063008392334,
                    "type": "normal"
                },
                {
                    "id": 10,
                    "text": "間奏",
                    "time": 68.01218610299682,
                    "type": "interlude",
                    "duration": 15.410545744415288
                },
                {
                    "id": 11,
                    "text": "誰も僕らの声は",
                    "time": 83.42273184741211,
                    "type": "normal"
                },
                {
                    "id": 12,
                    "text": "聞こえたりしない",
                    "time": 85.86184176730346,
                    "type": "normal"
                },
                {
                    "id": 13,
                    "text": "限りある命で呼びかけて諦めた言葉達に",
                    "time": 87.38384803051758,
                    "type": "normal"
                },
                {
                    "id": 14,
                    "text": "応えようも無いままに",
                    "time": 93.8010150114441,
                    "type": "normal"
                },
                {
                    "id": 15,
                    "text": "言いかけては三度挫けて爆ぜた",
                    "time": 97.06031897329711,
                    "type": "normal"
                },
                {
                    "id": 16,
                    "text": "捉えようのない想いも",
                    "time": 100.59136201907349,
                    "type": "normal"
                },
                {
                    "id": 17,
                    "text": "笑い事にならない、出来ない今日を",
                    "time": 103.85183810681153,
                    "type": "normal"
                },
                {
                    "id": 18,
                    "text": "思い返すにしても",
                    "time": 107.17956398855591,
                    "type": "normal"
                },
                {
                    "id": 19,
                    "text": "もう",
                    "time": 113.33429455368042,
                    "type": "normal"
                },
                {
                    "id": 20,
                    "text": "誰もいないのに",
                    "time": 117.07958984359742,
                    "type": "normal"
                },
                {
                    "id": 21,
                    "text": "幸せな夜の",
                    "time": 123.94067302288818,
                    "type": "normal"
                },
                {
                    "id": 22,
                    "text": "思い出も離ればなれだ。",
                    "time": 125.67867178256226,
                    "type": "normal"
                },
                {
                    "id": 23,
                    "text": "チェリーキャンディパイ",
                    "time": 129.85737114877318,
                    "type": "normal"
                },
                {
                    "id": 24,
                    "text": "甘い生地と猫",
                    "time": 131.7118088588562,
                    "type": "normal"
                },
                {
                    "id": 25,
                    "text": "歌う僕ら",
                    "time": 133.99144021743774,
                    "type": "normal"
                },
                {
                    "id": 26,
                    "text": "さよならの時は",
                    "time": 137.325657415802,
                    "type": "normal"
                },
                {
                    "id": 27,
                    "text": "こうやって",
                    "time": 138.88880712207032,
                    "type": "normal"
                },
                {
                    "id": 28,
                    "text": "先に眠ってくれないか",
                    "time": 140.63680190463256,
                    "type": "normal"
                },
                {
                    "id": 29,
                    "text": "忘れないで",
                    "time": 144.10175717166138,
                    "type": "normal"
                },
                {
                    "id": 30,
                    "text": "君のスカートが揺れてた",
                    "time": 154.44597201907348,
                    "type": "normal"
                },
                {
                    "id": 31,
                    "text": "淡い日差しに",
                    "time": 160.40296825177003,
                    "type": "normal"
                },
                {
                    "id": 32,
                    "text": "街を汚す夜に",
                    "time": 163.84180920980836,
                    "type": "normal"
                },
                {
                    "id": 33,
                    "text": "いつも通りに別れた",
                    "time": 167.6559240038147,
                    "type": "normal"
                },
                {
                    "id": 34,
                    "text": "上手くは言えないな。",
                    "time": 173.96485195422363,
                    "type": "normal"
                },
                {
                    "id": 35,
                    "text": "間奏",
                    "time": 178.07387210681154,
                    "type": "interlude",
                    "duration": 65.65887067956541
                },
                {
                    "id": 36,
                    "text": "愚かな僕ら",
                    "time": 243.73274278637695,
                    "type": "normal"
                },
                {
                    "id": 37,
                    "text": "街を抜け",
                    "time": 245.8817819732971,
                    "type": "normal"
                },
                {
                    "id": 38,
                    "text": "恋に溺れた",
                    "time": 247.5315499771118,
                    "type": "normal"
                },
                {
                    "id": 39,
                    "text": "窓ガラス、夜のベランダ",
                    "time": 250.08791304959107,
                    "type": "normal"
                },
                {
                    "id": 40,
                    "text": "なんだって消えないな",
                    "time": 252.30273295422364,
                    "type": "normal"
                },
                {
                    "id": 41,
                    "text": "奪い合う僕ら",
                    "time": 257.0277985956421,
                    "type": "normal"
                },
                {
                    "id": 42,
                    "text": "形あるものだけを信じて逃げた",
                    "time": 258.8633051296997,
                    "type": "normal"
                },
                {
                    "id": 43,
                    "text": "恐れてたんだ",
                    "time": 264.1002407673035,
                    "type": "normal"
                },
                {
                    "id": 44,
                    "text": "君のスカートが揺れてた",
                    "time": 274.11368506484985,
                    "type": "normal"
                },
                {
                    "id": 45,
                    "text": "欲深き夜に",
                    "time": 280.258440546051,
                    "type": "normal"
                },
                {
                    "id": 46,
                    "text": "9月の空に",
                    "time": 283.65288803433225,
                    "type": "normal"
                },
                {
                    "id": 47,
                    "text": "いつも通りに別れて",
                    "time": 287.26866667193605,
                    "type": "normal"
                },
                {
                    "id": 48,
                    "text": "また笑い合えたら",
                    "time": 293.64439477493283,
                    "type": "normal"
                },
                {
                    "id": 49,
                    "text": "君のスカートが揺れてた",
                    "time": 297.75736223651126,
                    "type": "normal"
                },
                {
                    "id": 50,
                    "text": "淡い昨日に",
                    "time": 303.6899151564026,
                    "type": "normal"
                },
                {
                    "id": 51,
                    "text": "街を汚す夜に",
                    "time": 307.15716023651123,
